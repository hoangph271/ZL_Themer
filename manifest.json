{
  "name": "Zalo Theemer",
  "manifest_version": 2,
  "author": "Microsoft Edge Extension Developer",
  "version": "1.0",
  "description": "Change the color of the body on docs.microsoft.com",
  "permissions": [
    "*://docs.microsoft.com/*",
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://chat.zalo.me/*"
      ]
    }
  ]
}